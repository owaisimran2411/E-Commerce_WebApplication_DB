<%- include('partials/header-seller.ejs') -%> 
     <br>
     
     <div id="page-content-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="">Your Available Products</h1><hr>
                    <table class="table  table-dark text-center" >
                        <thead>
                            <tr class="">
                                <th scope="col">Request Id</th>
                                <th scope="col">Product Name</th>
                                <th scope="col">Product Description</th>
                                <th scope="col">Product Type</th>
                                <th scope="col">Product Price</th>
                                <th scope="col">Product Price Type</th>
                                <th scope="col">Ad Status</th>
                                <th scope="col">Bids</th>
                               
                            </tr>
                          </thead>
                          <tbody>
                            <% data.forEach(element => { %>
                                <% if (element.AD_STATUS === 'approved') { %>
                                    <tr>
                                        <td><%= element.PRODUCT_ID %></td>
                                        <td><%= element.PRODUCT_NAME %></td>
                                        <td><%= element.PRODUCT_DESCRIPTION %></td>
                                        <td><%= element.PRODUCT_TYPE %></td>
                                        <td><%= element.PRODUCT_PRICE %></td>
                                        <td><%= element.PRODUCT_PRICE_TYPE %></td>
                                        <td><%= element.AD_STATUS %></td>
                                        
                                        <% if (element.PRODUCT_PRICE_TYPE == 'bidding price') { %>
                                            <td>
                                                <a class="btn btn-sm btn-primary" href="/view-bid-details/<%= element.PRODUCT_ID %> ">View Bids</a>
                                            </td>
                                        <% } %>
                                        <% if (element.PRODUCT_PRICE_TYPE == 'fixed price') { %>
                                            <td>N/A</td>
                                        <% } %>
                                    </tr>
                                <% } %>
                            <% }) %>
                        </tbody>
                      </table><br>
                      <h1>Your Pending Ads Request</h1><hr>
                      <!-- <h1>Your Pending Ads Request</h1> -->
                      <hr>
                      <table class="table table-dark">
                          <thead>
                              <th>Request Id</th>
                              <th>Product Name</th>
                              <th>Product Description</th>
                              <th>Product Type</th>
                              <th>Product Price</th>
                              <th>Product Price Type</th>
                              <th>Ad Status</th>
                          </thead>
                          <tbody>
                              <% data.forEach(element => { %>
                                  <% if (element.AD_STATUS === 'pending approval') { %>
                                      <tr>
                                          <td><%= element.PRODUCT_ID %></td>
                                          <td><%= element.PRODUCT_NAME %></td>
                                          <td><%= element.PRODUCT_DESCRIPTION %></td>
                                          <td><%= element.PRODUCT_TYPE %></td>
                                          <td><%= element.PRODUCT_PRICE %></td>
                                          <td><%= element.PRODUCT_PRICE_TYPE %></td>
                                          <td><%= element.AD_STATUS %></td>
                                      </tr>
                                  <% } %>
                              <% }) %>
                          </tbody>
                      </table>
                    <!-- <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Toggle Menu</a> -->
                </div>
            </div>
        </div>
    </div>
    <!-- /#page-content-wrapper -->

</div>


